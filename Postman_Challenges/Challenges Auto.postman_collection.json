							"});\r",
							"\r",
							"pm.test(\"Response is an object with todos property\", function () {\r",
							"    pm.expect(pm.response.json()).to.be.an('object').that.has.property('todos');\r",
							"});\r",
							"\r",
							"pm.test(\"Each element in the todos array has id, title, doneStatus, and description properties\", function () {\r",
							"    pm.response.json().todos.forEach(function(todo) {\r",
							"        pm.expect(todo).to.have.property('id');\r",
							"        pm.expect(todo).to.have.property('title');\r",
							"        pm.expect(todo).to.have.property('doneStatus');\r",
							"        pm.expect(todo).to.have.property('description');\r",
							"    });\r",
							"});"
