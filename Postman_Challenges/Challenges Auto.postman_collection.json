		{
			"name": "GET /challenges (200)",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Response status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Response is in JSON format\", function () {\r",
							"    pm.response.to.have.header(\"Content-Type\", \"application/json\");\r",
							"});\r",
							"\r",
							"pm.test(\"Response contains an array of challenges\", function () {\r",
							"    pm.expect(pm.response.json()).to.have.property('challenges').that.is.an('array');\r",
							"});\r",
							"\r",
							"pm.test(\"Each challenge in the array has required properties\", function () {\r",
							"    pm.response.json().challenges.forEach(function(challenge) {\r",
							"        pm.expect(challenge).to.have.property('id');\r",
							"        pm.expect(challenge).to.have.property('name');\r",
							"        pm.expect(challenge).to.have.property('description');\r",
							"        pm.expect(challenge).to.have.property('status');\r",
							"    });\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
